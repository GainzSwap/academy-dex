{
  "name": "academy-dex",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "account": "yarn workspace @academy-dex/backend-sol account",
    "chain": "yarn workspace @academy-dex/backend-sol chain",
    "compile": "yarn workspace @academy-dex/backend-sol compile",
    "deploy": "yarn workspace @academy-dex/backend-sol deploy",
    "flatten": "yarn workspace @academy-dex/backend-sol flatten",
    "fork": "yarn workspace @academy-dex/backend-sol fork",
    "format": "yarn next:format && yarn hardhat:format",
    "generate": "yarn workspace @academy-dex/backend-sol generate",
    "hardhat-verify": "yarn workspace @academy-dex/backend-sol hardhat-verify",
    "hardhat:format": "yarn workspace @academy-dex/backend-sol format",
    "hardhat:lint": "yarn workspace @academy-dex/backend-sol lint",
    "hardhat:lint-staged": "yarn workspace @academy-dex/backend-sol lint-staged",
    "hardhat:test": "yarn workspace @academy-dex/backend-sol test",
    "hardhat:coverage": "yarn workspace @academy-dex/backend-sol coverage",
    "postinstall": "husky install",
    "next:build": "yarn workspace @academy-dex/academy-dex-ui build",
    "next:check-types": "yarn workspace @academy-dex/academy-dex-ui check-types",
    "next:format": "yarn workspace @academy-dex/academy-dex-ui format",
    "next:lint": "yarn workspace @academy-dex/academy-dex-ui lint",
    "next:serve": "yarn workspace @academy-dex/academy-dex-ui serve",
    "precommit": "lint-staged",
    "start": "yarn workspace @academy-dex/academy-dex-ui dev",
    "test": "yarn hardhat:test",
    "vercel": "yarn workspace @academy-dex/academy-dex-ui vercel",
    "vercel:yolo": "yarn workspace @academy-dex/academy-dex-ui vercel:yolo",
    "verify": "yarn workspace @academy-dex/backend-sol verify"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.17.0"
  }
}
